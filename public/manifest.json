{
  "name": "Nautilus Wallet",
  "short_name": "Nautilus",
  "version": "0.3.2",
  "manifest_version": 2,
  "minimum_chrome_version": "48",
  "description": "Privacy Wallet Designed for Ergo Network",
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'wasm-eval'; object-src 'self';",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Nautilus Wallet"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["file://*/*", "http://*/*", "https://*/*"],
      "js": ["js/injector.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
    "48": "icons/app/48.png",
    "128": "icons/app/128.png",
    "512": "icons/app/512.png"
  }
}
